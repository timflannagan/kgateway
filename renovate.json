{
    "extends": [
        "config:base"
    ],
    "prBodyNotes": [
        "# Description",
        "",
        "Automated dependency update generated by Renovate.",
        "",
        "# Change Type",
        "",
        "/kind cleanup",
        "",
        "# Changelog",
        "",
        "```release-note",
        "NONE",
        "```"
    ],
    "labels": [
        "dependencies",
        "renovate"
    ],
    "commitMessagePrefix": "chore(deps): ",
    "prTitle": "chore(deps): {{depName}} to {{newVersion}}",
    "packageRules": [
        {
            "matchManagers": [
                "gomod"
            ],
            "groupName": "Go modules"
        },
        {
            "matchManagers": [
                "dockerfile"
            ],
            "groupName": "Docker images"
        },
        {
            "matchManagers": [
                "github-actions"
            ],
            "groupName": "GitHub Actions"
        }
    ]
}
