{
    "extends": [
        "config:base"
    ],
    "prHeader": "# Description\\n\\nAutomated dependency update generated by Renovate.\\n\\n# Change Type\\n\\n/kind cleanup\\n\\n# Changelog\\n\\n```release-note\\nNONE\\n```\\n",
    "prBodyNotes": [
        "# Additional Notes\\n\\n_This PR was created automatically by Renovate and will be re-generated on every rebase._"
    ],
    "labels": [
        "dependencies",
        "renovate"
    ],
    "commitMessagePrefix": "chore(deps): ",
    "prTitle": "chore(deps): {{depName}} to {{newVersion}}",
    "packageRules": [
        {
            "matchManagers": [
                "gomod"
            ],
            "groupName": "Go modules"
        },
        {
            "matchManagers": [
                "dockerfile"
            ],
            "groupName": "Docker images"
        },
        {
            "matchManagers": [
                "github-actions"
            ],
            "groupName": "GitHub Actions"
        }
    ]
}
